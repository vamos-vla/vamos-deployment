<launch>
  <!-- Load parameters from YAML -->
  <rosparam command="load" file="$(find vlm_ros)/config/navigate.yaml" />
  
  <!-- Launch the VLM navigation node -->
  <node name="vlm_navigation_node" pkg="vlm_ros" type="navigate.py" output="screen" respawn="true">
    <!-- All parameters are loaded from the YAML file -->
  </node>
</launch>